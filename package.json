{
  "name": "nebuia-ts",
  "version": "2.0.10",
  "description": "Common typescript libraries for Nebuia",
  "keywords": [],
  "author": "Ulises Rafael Ramírez Martínez",
  "license": "ISC",
  "module": "dist/index.js",
  "types": "dist/types",
  "main": "dist/index.cjs",
  "files": [
    "dist/*",
    "README.md",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "build": "rimraf dist && rollup --bundleConfigAsCjs -c ./rollup.config.mjs",
    "lint": "pnpm run lint:eslint && pnpm run lint:typescript",
    "lint:eslint": "eslint --fix --max-warnings=0 --ext .ts .",
    "lint:typescript": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepare": "pnpm run build"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@types/jest": "29.5.12",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-codely": "3.1.4",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.29.1",
    "rimraf": "5.0.10",
    "rollup": "4.34.8",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "axios": "1.7.9",
    "form-data": "4.0.2"
  },
  "peerDependencies": {
    "axios": "^1.7.9",
    "form-data": "^4.0.2"
  }
}
