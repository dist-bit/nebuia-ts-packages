{
  "name": "nebuia-ts",
  "version": "1.0.0",
  "description": "Common typescript libraries for Nebuia",
  "keywords": [],
  "author": "Ulises Rafael Ramírez Martínez",
  "license": "ISC",
  "module": "dist/index.js",
  "types": "dist/types",
  "main": "dist/index.cjs",
  "files": [
    "dist/*",
    "README.md",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "lint": "pnpm exec --recursive --parallel lint",
    "publish:all": "pnpm recursive exec -- pnpm publish --access public",
    "build": "rimraf dist && rollup --bundleConfigAsCjs -c ./rollup.config.mjs",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "dotenv": "16.4.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-codely": "3.1.3",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "rollup": "4.9.6",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "axios": "1.6.7",
    "form-data": "4.0.0"
  },
  "peerDependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  }
}
